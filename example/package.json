{
  "name": "example",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "setup": "yarn && (cd node-site && yarn) && (cd worker-site && yarn)",
    "dev": "vitedge dev --force",
    "build": "rm -rf dist && vitedge build",
    "refresh": "rm -rf node_modules/vitedge && cp -r ../core node_modules/vitedge && cp ../core/bin/cli.js node_modules/.bin/vitedge",
    "serve:worker": "cd worker-site && yarn serve",
    "serve:node": "cd node-site && yarn serve"
  },
  "dependencies": {
    "vite": "^2.0.0-beta.69",
    "vitedge": "file:../core",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.25",
    "@vitejs/plugin-vue": "^1.1.4",
    "@vue/compiler-sfc": "^3.0.5",
    "@vuedx/typescript-plugin-vue": "^0.6.3",
    "ts-node": "^9.1.0",
    "typescript": "^4.1.2"
  }
}